
class point
   {
   float x,y;
   to move(float dx, dy) { self.x += dx; self.y += dy;}
   }

point pt = (1,2);
print(pt);

to printPoint(point pt)
{
print(pt);
}

class coloredPoint extends point
   {
   string color;
   to recolor(string newColor) {self.color = newColor;}
   }
coloredPoint cp = (1, 2, "red");
print(cp);
cp.move(10,20);
