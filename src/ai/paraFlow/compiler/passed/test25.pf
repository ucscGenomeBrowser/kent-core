to freen(float x) into (float square, int mod4)
    {
    int i = x;
    square = x*x;
    mod4 = i&3;
    }

to square(float a) into float result
   {
   result = a*a;
   }

class point
   {
   int x,y;
   to shift(int dx, int dy)
       {
       x += dx;
       y += dy;
       }
   }

class box
/* A rectangle in 2 D */
    {
    float x,y;
    float width, height;
    };

