class point
   {
   string name;
   int x,y,z=3;
   }

class line
   {
   point start;
   point end;
   }

point jim = ("JIM", 1, 2, 3);
point heidi = ("HEIDI", 10, 20, 30);
line line = (("Jim", 1, 2, 3),("Heidi", 10, 20, 30));
print(line.start);
print(line.end);
print(line);
line.start = jim;
line.end = heidi;
print(line);
line.start = ("_jim_", 1, 2, 3);
line.end = ("_heidi_", 10, 20, 30);
print(line);

