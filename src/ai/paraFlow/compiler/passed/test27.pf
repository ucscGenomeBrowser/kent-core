to swap(int x, int y) into (int xx, int yy)
{
xx = y;
yy = x;
}

to double2(int x, int y) into (int x2, int y2)
{
x2 = x+x;
y2 = y+y;
}

to double(int x) into int y
{
y = x+x;
}

to square(int x) into int y
{
y = x*x;
}

