
class animal
// Base clase for animals
    {
    flow name() into string name {name = "animal";}
    }
animal animal;

class zebra 
    {
    flow name() into string name {name = "zebra";}
    }
zebra zebra;


to printInfo(animal a)
    {
    print(a.name());
    }

printInfo(animal);
printInfo(zebra);
