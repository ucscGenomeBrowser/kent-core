
class point
   {
   int x,y;
   to move(int dx,dy)
       {
       x += dx;
       y += dy;
       }
    to incX()
       {
       move(1,0);
       }
    }

class fatPoint extends point
    {
    int radius = 2;
    to dink()
        {
	move(1,2);
	}
    to boing()
        {
	x += 1;
	y -= 1;
	}
    }

fatPoint f = (1,2,3);
print(f);
f.dink();
print(f);
f.incX();
print(f);
f.boing();
print(f);
