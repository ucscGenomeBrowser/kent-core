
array of double d = (2,1,3,4,5,4.5);
print(d);
long l = para (x in d) argmax x*x;
print("argmax: " + l);
l = para(x in d) argmin x*x;
print("argmin: " + l);

l = para(x in -5 til 6) argmin x*x;
print("argmin of x*x in -5 to 6: " + l);

dir of int numNames = (one @ 1, two @ 2, zero @ 0, minusOne @ -1);
print(numNames);
string s = para (x in numNames) argmin x*x;
print("argmin x*x: " + s);
string t = para (x in numNames) argmax x;
print("argmax x: " + t);

