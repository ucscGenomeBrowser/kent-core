table wabaCrude
"Info on a crude alignment"
    (
    uint score;  "score 0 to about 6000"
    string qFile;  "query sequence file"
    string qSeq;   "query sequence within file"
    uint qStart;   "query start position"
    uint qEnd;     "query end position"
    byte strand;   "strand of alignment: +1 or -1"
    string tFile;  "target sequence file"
    string tSeq;   "target sequence within file"
    uint tStart;   "target start position"
    uint tEnd;  "target end position"
    )
