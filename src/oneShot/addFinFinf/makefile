kentSrc = ../..
include ../../inc/common.mk


L += -lm
MYLIBDIR = ../../lib/$(MACHTYPE)
MYLIBS =  $(MYLIBDIR)/jkhgap.a $(MYLIBDIR)/jkweb.a

O = addFinFinf.o

addFinFinf: $O $(MYLIBS)
	${CC} ${COPT} ${CFLAGS} -o ${BINDIR}/addFinFinf $O $(MYLIBS) $L
	${STRIP} ${BINDIR}/addFinFinf${EXE}

clean::
	rm -f ${O}
